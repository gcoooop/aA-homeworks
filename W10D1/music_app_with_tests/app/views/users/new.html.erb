<h2>Sign up!</h2>

<form action="<%= users_url %>" method="POST">
  <%= auth_token %>
  
  <label for="email">Email:</label>
  <input type="text" name="user[email]">

  <label for="password">Password:</label>
  <input type="password" name="user[password]">

  <input type="submit" value="Register">

</form>